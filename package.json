    "scripts": {
        "start": "node scripts/start.js",
        "build": "cross-env REACT_APP_BACKEND_URL=reaql-backend.herokuapp.com/graphql node scripts/build.js",
        "test": "NODE_PATH=./src jest",
        "test:watch": "NODE_PATH=./src jest --watchAll",
        "test:coverage": "NODE_PATH=./src jest --runInBand --coverage --collectCoverageFrom=src/**/*.{ts,tsx} --collectCoverageFrom=!src/**/*.stories.{ts,tsx} --collectCoverageFrom=!src/lib/**/* --collectCoverageFrom=!src/config/**/* --collectCoverageFrom=!src/lib --collectCoverageFrom=!src/**/*.{types,d}.{ts,tsx}",
        "test:coverage:watch": "npm run test:coverage --watchAll",
        "storybook": "start-storybook -p 6006 -s public",
        "build-storybook": "build-storybook -s public",
        "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "pretify": "prettier --config=./.prettierrc --write ./src",
        "codegen": "graphql-codegen",
        "serve": " serve -s build -p ${PORT:=3000}",
        "heroku-prebuild": "npm install -g serve"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
