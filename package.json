{
    "name": "with-typescript-graphql",
    "version": "0.1.0",
    "author": "",
    "license": "ISC",
    "scripts": {
        "codegen": "yarn graphql-codegen",
        "dev": "next",
        "dev-codegen": "yarn codegen && next",
        "build": "yarn codegen && next build",
        "start": "next start",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "test": "jest",
        "test:watch": "jest --watchAll",
        "test:coverage": "yarn test --runInBand --coverage --collectCoverageFrom=src/**/*.{ts,tsx} --collectCoverageFrom=!src/**/*.stories.{ts,tsx} --collectCoverageFrom=!src/lib/**/* --collectCoverageFrom=!src/config/**/* --collectCoverageFrom=!src/lib --collectCoverageFrom=!src/**/*.{types,d}.{ts,tsx}",
        "test:coverage:watch": "yarn test:coverage --watchAll",
        "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
        "pretify": "prettier --config=./.prettierrc --write ./src"
    },
    "dependencies": {
        "@apollo/client": "^3.1.4",
        "@apollo/react-common": "3.1.4",
        "@apollo/react-components": "^3.1.5",
        "@babel/runtime": "^7.11.2",
        "@svgr/webpack": "^5.4.0",
        "apollo-cache": "^1.3.5",
        "apollo-cache-inmemory": "^1.6.6",
        "apollo-client": "^2.6.10",
        "apollo-link": "1.2.14",
        "apollo-link-http": "1.5.17",
        "apollo-link-schema": "1.2.5",
        "apollo-server-micro": "^2.14.2",
        "apollo-utilities": "^1.3.3",
        "date-fns": "^2.16.1",
        "emoji-picker-react": "^3.2.4",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.3",
        "graphql": "^14.6.0",
        "jest": "^26.4.2",
        "next": "latest",
        "react": "^16.13.1",
        "react-apollo": "^3.1.5",
        "react-dom": "^16.13.1",
        "styled-components": "^5.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.11.4",
        "@babel/plugin-transform-runtime": "^7.11.0",
        "@graphql-codegen/cli": "^1.17.8",
        "@graphql-codegen/plugin-helpers": "^1.15.1",
        "@graphql-codegen/typescript": "^1.13.3",
        "@graphql-codegen/typescript-document-nodes": "^1.17.8",
        "@graphql-codegen/typescript-operations": "^1.13.3",
        "@graphql-codegen/typescript-react-apollo": "^2.0.6",
        "@graphql-codegen/typescript-resolvers": "^1.15.1",
        "@storybook/addon-actions": "^6.0.27",
        "@storybook/addon-essentials": "^6.0.27",
        "@storybook/addon-links": "^6.0.27",
        "@storybook/react": "^6.0.27",
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^26.0.10",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "@types/styled-components": "^5.1.2",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "babel-loader": "^8.1.0",
        "babel-plugin-styled-components": "^1.11.1",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
        "enzyme-to-json": "^3.5.0",
        "eslint": "^7.7.0",
        "eslint-plugin-react": "^7.20.6",
        "graphql-let": "0.x",
        "husky": "^4.2.5",
        "jest-styled-components": "^7.0.3",
        "react-is": "^16.13.1",
        "regenerator-runtime": "^0.13.7",
        "ts-loader": "^8.0.3",
        "typescript": "^3.8.3"
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts",
            "!src/**/*.stories.{ts,tsx}"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.ts"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [
            "<rootDir>/src"
        ],
        "moduleNameMapper": {
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/jest/fileTransform.js"
        },
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint",
            "pre-push": "yarn test"
        }
    }
}
